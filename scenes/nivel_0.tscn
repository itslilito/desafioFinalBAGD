[sub_resource type="GDScript" id="GDScript_53nwt"]
script/source = "extends StaticBody2D

@export var animacion : AnimatedSprite2D
@export var animacion_desvanecer : AnimationPlayer

func _process(delta):
	animacion.play(\"idle\")

func colision_jugador(cuerpo):
	if cuerpo.name == \"murciegalito\":
		if cuerpo.llave_1 > 0:
			cuerpo.llave_1 = cuerpo.llave_1 -1
			animacion_desvanecer.play(\"desvanecer\")
			await get_tree().create_timer(2.0).timeout
			queue_free()
"

